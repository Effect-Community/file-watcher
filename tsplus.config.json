{
    "importMap": {
        "^(.*)/packages/([^/]*)/src(.*)/index\\.ts$": "@effect-ts/$2$3",
        "^(.*)/packages/([^/]*)/src(.*)\\.ts$": "@effect-ts/$2$3",
        "^(.*)/packages/([^/]*)/test/(.*)\\.ts$": "@effect-ts-tests/$2/$3"
    },
    "traceMap": {
        "^(.*)/packages/([^/]*)/(.*)$": "(@effect-ts/$2) $3"
    }
}
